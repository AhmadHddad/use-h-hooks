{
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "npx typedoc src/index.ts"
  },
  "keywords": [
    "hooks",
    "AhmadHddad",
    "use-h-hooks",
    "useHooks",
    "react"
  ],
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": "https://github.com/AhmadHddad/use-h-hooks",
  "name": "use-h-hooks",
  "author": "AhmadHddad",
  "module": "dist/use-h-hooks.esm.js",
  "size-limit": [
    {
      "path": "dist/use-h-hooks.cjs.production.min.js",
